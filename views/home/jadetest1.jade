extends ../layout

block content

    div.parallax
      div.water.water-layer4
      div.water.water-layer3
      div.water.water-layer2

    div.equalmargin

      div#widgets.packery.fluid.has-padding.has-stamp

        div.stamp
          p This is stamp content

        div.item.w4.h4
          include ../modules/holon_data.html
          img(src='../images/mock-07.png')

        div.item.w4.h2.text-center.draggable
          p
          h1 Sign up
   
        div.item.w4.h2.text-center.draggable
          div.togglestamp <i class="fa fa-level-up"></i>
          div.movehandle <i class="fa fa-plus"></i>
          p
          h1 Sign in

        div.item.w4.h4.draggable.fixedsize
          div.text-left.pull-left.col-xs-6.nopadding
            p
            h2 Zug
          div.text-left.pull-left.col-xs-5.nopadding
            p
            h3 +1C
            h4 -3
          div.text-left.pull-left.col-xs-6.nopadding
            p
            <i class="fa fa-cloud fa-5x"></i>
          div.text-center.pull-left.col-xs-12.nopadding
            h3 15:08 GMT+1
        
        div.item.w8.h2.draggable
          include ../modules/startpage_search.html

        div.item.w4.h4.draggable
          img(src='../images/mock-28.png')

        div.item.w8.h6.draggable
          img(src='../images/mock-14.png')

        div.item.w4.h2.draggable
          include ../modules/bitcoinaverage_price.html

        div.item.w4.h6.draggable
          img(src='../images/mock-13.png')
        
        div.item.w4.h2.draggable
          img(src='../images/mock-11.png')
        
        div.item.w4.h4.draggable
          include ../modules/soundcloud.html

        div.item.w4.h8.draggable
          img(src='../images/mock-15.png')

        div.item.w4.h2.draggable
          img(src='../images/mock-10.png')

        div.item.w8.h8.draggable.morecontent
          p #{brand}

